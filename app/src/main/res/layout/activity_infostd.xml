<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/textViewinscrip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Inscription"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textViewfname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Name"
        app:layout_constraintTop_toTopOf="@id/textViewinscrip"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="20dp"

        />

    <EditText
        android:id="@+id/editTextTextFname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintTop_toBottomOf="@id/textViewinscrip"
        app:layout_constraintStart_toStartOf="@id/textViewinscrip"
        app:layout_constraintEnd_toEndOf="@id/textViewinscrip"
        android:layout_marginTop="45dp"



        />

    <TextView
        android:id="@+id/textViewLname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Last Name"
        app:layout_constraintTop_toBottomOf="@id/textViewfname"
        app:layout_constraintStart_toStartOf="@id/textViewfname"
        app:layout_constraintEnd_toEndOf="@id/textViewfname"
        android:layout_marginTop="65dp"

        />

    <EditText
        android:id="@+id/editTextTextLname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintTop_toBottomOf="@id/editTextTextFname"
        app:layout_constraintStart_toStartOf="@id/editTextTextFname"
        app:layout_constraintEnd_toEndOf="@id/editTextTextFname"
        android:layout_marginTop="45dp"
        />

    <TextView
        android:id="@+id/textViewid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I D"
        app:layout_constraintTop_toBottomOf="@id/textViewLname"
        app:layout_constraintStart_toStartOf="@id/textViewLname"
        app:layout_constraintEnd_toEndOf="@id/textViewLname"
        android:layout_marginTop="70dp"
        />

    <EditText
        android:id="@+id/editTextTextID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintTop_toBottomOf="@id/editTextTextLname"
        app:layout_constraintStart_toStartOf="@id/editTextTextLname"
        app:layout_constraintEnd_toEndOf="@id/editTextTextLname"
        android:layout_marginTop="45dp"

        />

    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        app:layout_constraintTop_toBottomOf="@id/textViewid"
        app:layout_constraintStart_toStartOf="@id/textViewid"
        app:layout_constraintEnd_toEndOf="@id/textViewid"
        android:layout_marginTop="75dp"
        />

    <EditText
        android:id="@+id/editTextTextEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintTop_toBottomOf="@id/editTextTextID"
        app:layout_constraintStart_toStartOf="@id/editTextTextID"
        app:layout_constraintEnd_toEndOf="@id/editTextTextID"
        android:layout_marginTop="45dp"
        />

    <Button
        android:id="@+id/buttonAVG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Print"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="20dp"
        />

    <Button
        android:id="@+id/buttonClear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="clear"
        app:layout_constraintTop_toTopOf="@id/buttonAVG"
        app:layout_constraintBottom_toBottomOf="@id/buttonAVG"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@id/buttonAVG"

        />

    <Button
        android:id="@+id/buttonReturn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Return"
        app:layout_constraintTop_toTopOf="@id/buttonClear"
        app:layout_constraintBottom_toBottomOf="@id/buttonClear"
        android:layout_marginLeft="30dp"
        app:layout_constraintLeft_toRightOf="@id/buttonClear"

        />


</androidx.constraintlayout.widget.ConstraintLayout>